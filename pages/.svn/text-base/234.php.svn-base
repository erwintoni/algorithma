INSERT(int x, Node root)
{   
  if(find(x))
	searchInsert(root,x);
}

void searchInsert(node,x)
{
  if(node.links == 3)
	node = split(node);
  if(node.links == 0) 
	node.add(x);
  else
  	search(node.Currentlink)
}
       
bool find(int value)
{
  if(Binary_Search(value));
	return true;

  return false;
