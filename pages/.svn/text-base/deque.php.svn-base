<pre>
//Functions for a Deque in C++ 
//with a double vector (dynamic array) implementation.
//vecOne and vecTwo are std::Vector<T>

void Deque<T>::push_front(const T & value)
{
  vecOne.push_back(value);
}//push_front(T)

void Deque<T>::push_back(const T & value)
{
  vecTwo.push_back(value);
}//push_back(T)

void Deque<T>::pop_front()
{
  if(!vecOne.empty())
	vecOne.pop_back();
  else
	vecTwo.erase(vecTwo.begin());
}//pop_front()

void Deque<T>::pop_back()
{
  if(!vecTwo.empty())
	vecTwo.pop_back();
  else
	vecOne.erase(vecOne.begin());
}//pop_back()
