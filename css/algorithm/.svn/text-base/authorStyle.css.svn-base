{	
	width: 32%;
	vertical-align: middle;
}

#presets
{
	float: right;
}

.work_area
{
	position: relative;
}   
.plain-box-style
{
	position: absolute;
	padding: 0;
}

.box
{
    background-color:#0000E0;
    width:45px;
    height:45px;
    font-weight:bold;
    font-size:35px;
    text-align:center;
    color:white;
    vertical-align:middle;
    display:table-cell;
    position:absolute;
    padding: 0;
}

.pivot
{
    width:45px;
    height:45px;
    font-weight:bold;
    font-size:20px;
    text-align:center;
    vertical-align:bottom;
    display:table-cell;
    position:absolute;
    padding: 0;
	z-index: 1;
}

